# Generated by Chef for <%= node['fqdn'] %>

<% @settings.each do |key, value| %>
<%  if key == "match" && value.kind_of?(Array) %>
<%    value.each do |mg| %>
<%      mg.each do |k, v| %>
<%=       "#{k.split("_").map { |w| w.capitalize}.join} #{v}" %>
<%      end %>
<%    end %>
<%  elsif key != "match" && value.kind_of?(Array) %>
<%    value.each do |item| %>
<%=     "#{key.split("_").map { |w| w.capitalize}.join} #{item}" %>
<%    end %>
<%  else %>
<%=   "#{key.split("_").map { |w| w.capitalize}.join} #{value}" %>
<%  end %>
<% end %>
